{
  "github_client_id": "<GITHUB_CLIENT_ID>",
  "github_client_secret": "<GITHUB_CLIENT_SECRET>",
  "github_oauth_login_url": "https://github.com/login/oauth/authorize",
  "github_token_url": "https://github.com/login/oauth/access_token",
  "token_file": "/etc/puppetlabs/github_oauth_token",
  "token_expiry_file": "/etc/puppetlabs/github_oauth_token_expiry",
  "refresh_token_file": "/etc/puppetlabs/github_oauth_refresh_token",
  "refresh_threshold": 14400,
  "callback_uri": "https://127.0.0.1/callback",
  "scope": "repo"
}